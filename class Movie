public class Movie {
    String title;
    String director;
    String genre;
    int releaseYear;
    double rating; // 0.0 - 10.0

    public Movie(String title, String director, String genre, int releaseYear, double rating) {
        this.title = title;
        this.director = director;
        this.genre = genre;
        this.releaseYear = releaseYear;
        this.rating = rating;
    }

}
